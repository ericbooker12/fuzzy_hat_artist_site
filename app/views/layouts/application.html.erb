<!DOCTYPE html>
<html>
  <head>
    <title>FuzzyHatArtistSite</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="nav">
        <% if user_signed_in? %>
          <%= link_to "Log out", destroy_user_session_path, method: :delete, alt: "Log out" %>
        <% end %>
      </div>
      <p class="notice"><%= notice %></p>
      <%= yield %>
    </div>
  </body>
</html>
